        <Treatment id="1750" parentID="1928" level="4" creatorID="0" sortOrder="1" createDate="2014-08-12T23:47:16" updateDate="2014-09-27T08:58:54" nodeName="Nipple And Areola Procedures" urlName="nipple-and-areola-procedures" path="-1,1568,1994,1928,1750" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
