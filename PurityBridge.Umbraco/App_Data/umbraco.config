        <Treatment id="1944" parentID="1927" level="4" creatorID="0" sortOrder="2" createDate="2014-09-03T21:35:52" updateDate="2014-09-27T08:58:44" nodeName="Skin tag removal" urlName="skin-tag-removal" path="-1,1568,1994,1927,1944" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
