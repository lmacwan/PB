        <Treatment id="1929" parentID="1928" level="4" creatorID="0" sortOrder="0" createDate="2014-09-03T20:43:14" updateDate="2014-09-27T08:58:54" nodeName="Scar revision" urlName="scar-revision" path="-1,1568,1994,1928,1929" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
