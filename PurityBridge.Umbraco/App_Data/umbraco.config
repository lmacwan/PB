      <Treatment id="1709" parentID="1506" level="3" creatorID="0" sortOrder="3" createDate="2014-08-12T23:22:33" updateDate="2014-08-15T01:19:33" nodeName="Sclerotherapy" urlName="sclerotherapy" path="-1,1568,1506,1709" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
