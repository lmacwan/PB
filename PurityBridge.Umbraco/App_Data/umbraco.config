      <Treatment id="1750" parentID="1506" level="3" creatorID="0" sortOrder="6" createDate="2014-08-12T23:47:16" updateDate="2014-08-15T01:19:34" nodeName="Labial Reduction" urlName="labial-reduction" path="-1,1568,1506,1750" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
