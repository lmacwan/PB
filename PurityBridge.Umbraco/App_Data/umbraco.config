          <SmartBlogPost id="3618" parentID="3191" level="5" creatorID="2" sortOrder="1" createDate="2014-12-09T20:56:54" updateDate="2014-12-20T19:54:24" nodeName="CCR-Expo report – inc. MDP approach – aesthetic polyclinic" urlName="ccr-expo-report-inc-mdp-approach-aesthetic-polyclinic" path="-1,3168,3169,3190,3191,3618" isDoc="" nodeType="3163" creatorName="Richard Christian" writerName="Richard Christian" writerID="2" template="3154" nodeTypeAlias="SmartBlogPost">
