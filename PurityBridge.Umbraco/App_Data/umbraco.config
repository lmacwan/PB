      <Treatment id="1764" parentID="1506" level="3" creatorID="0" sortOrder="7" createDate="2014-08-12T23:52:43" updateDate="2014-08-15T01:19:34" nodeName="Scar Revision" urlName="scar-revision" path="-1,1568,1506,1764" isDoc="" nodeType="1477" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="1513" nodeTypeAlias="Treatment">
