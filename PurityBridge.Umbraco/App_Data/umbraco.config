    <ClinicData id="2220" parentID="1568" level="2" creatorID="0" sortOrder="1" createDate="2014-09-18T20:54:26" updateDate="2015-12-04T16:14:45" nodeName="Clinic" urlName="clinic" path="-1,1568,2220" isDoc="" nodeType="2219" creatorName="Leon Macwan" writerName="Leon Macwan" writerID="0" template="5869" nodeTypeAlias="ClinicData">
