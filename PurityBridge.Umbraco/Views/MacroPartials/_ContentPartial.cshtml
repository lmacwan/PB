@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{
    ViewBag.Umbraco = Umbraco;
}


@foreach (IPublishedContent child in Model.Content.Children(c => c.Children.Count() > 0))
{
    if (child.DocumentTypeAlias == "Columns")
    {
    @Html.Partial("ColumnsPartial", child)
    }
    else if (child.DocumentTypeAlias == "List")
    {
    @Html.Partial("_ListPartial", child)
    }
}
@Html.Partial("_ChildrenPartial", Model.Content)