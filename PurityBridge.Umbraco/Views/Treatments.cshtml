@using Umbraco.Core.Models
@inherits Umbraco.Web.Mvc.UmbracoViewPage<Umbraco.Web.Models.RenderModel>
@{
    Layout = "/Views/Shared/_Layout.cshtml";
    ViewBag.UrlPrefix = string.Empty;
    IPublishedContent treatmentsData = Model.Content;
}
<style>
    #photo-content {
        margin-top: 30px;
    }
</style>
@if (treatmentsData != null)
{
    @Html.Partial("_HighlightsPartial", treatmentsData.Descendants("TreatmentsCategoryData"));

    <div id="photo-content" class="vc_row-fluid beautify">
        @Html.Raw(treatmentsData.GetPropertyValue<string>("pageTextContent"))
    </div>
}
