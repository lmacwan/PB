@using Umbraco.Core.Models
@inherits Umbraco.Web.Mvc.UmbracoViewPage<Umbraco.Web.Models.RenderModel>
@{
    Layout = "/Views/Shared/_Layout.cshtml";
    ViewBag.ShowTitleBar = false;
    var rightChoiceData = uQuery.GetNodesByType("RightChoiceData") ?? new List<umbraco.NodeFactory.Node>();
    ViewBag.Crop = "Rectangular";
}
<style>    
    .spannedItems .left .align,
    .spannedItems .right .align     
    {
        float: right;
        margin-left: 20px;
    }
</style>

@Html.Partial("_TextContent", rightChoiceData)