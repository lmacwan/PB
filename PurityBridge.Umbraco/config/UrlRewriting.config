<?xml version="1.0" encoding="utf-8"?>
<urlrewritingnet xmlns="http://www.urlrewriting.net/schemas/config/2006/07">
  <rewrites>
    <add name="umbracorewrite"
                  virtualUrl="~/umbraco/"
                  rewriteUrlParameter="ExcludeFromClientQueryString"
                  destinationUrl="/umbraco/"
                  ignoreCase="true" />

    <add name="treatmentdetailrewrite"
              virtualUrl="~/(?!(umbraco))([a-z\-]{1,})/"
              rewriteUrlParameter="ExcludeFromClientQueryString"
              destinationUrl="/treatments/$2/"
              ignoreCase="true" />

    <add name="treatmentphotosrewrite"
              virtualUrl="~/(?!(umbraco))([a-z\-]{1,})/photos"
              rewriteUrlParameter="ExcludeFromClientQueryString"
              destinationUrl="/treatments/$2/photos"
              ignoreCase="true" />    

    <add name="homerewrite"
           virtualUrl="/(?!(umbraco)|([a-z\-]{1,}))"
           rewriteUrlParameter="ExcludeFromClientQueryString"
           destinationUrl="~/home/index"
           ignoreCase="true" />
  </rewrites>
</urlrewritingnet>
